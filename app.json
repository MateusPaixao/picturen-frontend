{
  "expo": {
    "name": "Picturen",
    "slug": "picturen-frontend",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#151728"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
       "bundleIdentifier": "com.fjordtech.picturen",
       "buildNumber": "1.0.0"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "themeColor": "#151728"
    },
    "android": {
      "package": "com.fjordtech.picturen",
      "versionCode": 1
    },

    "androidNavigationBar": {
      "barStyle": "light-content",
      "backgroundColor": "#151728"
    }
  }
}
